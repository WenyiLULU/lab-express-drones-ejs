<h2>Available drones</h2>
<ul>
    <% drones.forEach(({name, propellers, speed}) => { %>
        <li>
            <p><strong>Name: </strong><%= name %></p>
            <p><strong>Propellers: </strong><%= propellers %></p>
            <p><strong>Speed: </strong><%= speed %></p>
        </li>
    <% }) %>
</ul>